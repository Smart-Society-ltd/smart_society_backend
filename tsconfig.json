{
    "compilerOptions": {                    
      "module": "NodeNext", 
      "moduleResolution": "NodeNext",
      "target": "ES2022",
      "sourceMap": false,                 
      "outDir": "dist", 
      "baseUrl": "./src",
    "paths": {
      "@controllers/*": ["./Controllers/*"],
      "@models/*": ["./Models/*"],
      "@functions/*": ["./Functions/*"],
      "@routes/*": ["./Routes/*"]
    }                     
    },
    "include": ["src/**/*.ts"],       
    "exclude": ["node_modules"]           
  }