{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ES2022",
    "sourceMap": false,
    "outDir": "dist",
    "baseUrl": "./src",
    "paths": {
      "@Controllers/*": ["./Controllers/*"],
      "@Models/*": ["./Models/*"],
      "@Functions/*": ["./Functions/*"],
      "@Routes/*": ["./Routes/*"]
    }
  },
  "include": ["src/**/*.ts", "src/Routes/SocietyRoutes"],
  "exclude": ["node_modules"]
}
